{
  "name": "concrete5/composer",
  "description": "concrete5 composer based installation",
  "type": "project",
  "license": "MIT",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "concrete5/concrete5": "^8.0",
    "illuminate/container": "5.2.*"
  },
  "scripts": {
    "post-update-cmd": [
      "cp -af vendor/concrete5/concrete5/concrete ./"
    ],
    "post-install-cmd": [
      "cp -af vendor/concrete5/concrete5/concrete ./"
    ]
  },
  "extra": {
    "branch-alias": {
      "dev-master": "8.x-dev"
    }
  }
}
